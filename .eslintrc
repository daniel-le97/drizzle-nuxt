{
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "@nuxtjs/eslint-config-typescript"
    ],
    "overrides": [
      {
        "files": [
          "./server/**/*.ts"
        ],
        "rules": {
        "no-unused-vars":"off",
        "@typescript-eslint/no-unused-vars": "off",
        "no-console": "off"
